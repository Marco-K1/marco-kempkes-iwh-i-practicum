doctype html
html
    head
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        a(href="/update-cobj") Add/Update this table
        .cards
            .card
                h1 Custom Object Table
                table
                    thead
                        tr
                            th Name
                            th Region
                            th Price
                    each cheese in cheeses
                        tr
                            td #{cheese.properties.name}
                            td #{cheese.properties.region}
                            td #{cheese.properties.price}